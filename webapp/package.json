{
  "name": "@frederic/react-components",
  "version": "1.0.4",
  "description": "",
  "keywords": [],
  "main": "lib/index.js",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://gitlab.digital.homeoffice.gov.uk/frederic/webapp/"
  },
  "private": true,
  "watch": {
    "test": "./src/**/*.test.js"
  },
  "scripts": {
    "start": "npm run storybook",
    "build": "babel --source-maps -d lib/ src/",
    "watch": "babel --source-maps -d lib/ src/ -w",
    "build-storybook": "build-storybook",
    "docker": "npm run docker:build",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:test": "docker-compose run web npm run test-unit",
    "eslint": "eslint src --report-unused-disable-directives",
    "eslint:disabled": "eslint src --no-inline-config -f codeframe",
    "eslint:fix": "eslint src --fix",
    "eslint:rules": "eslint --print-config src/index.js",
    "make-index": "make-index -d src/components",
    "prepare": "npm run build",
    "sonarlint": "sonarlint --src \"src/**.js\"",
    "sonarlint:open": "if node -p 'process.platform' = 'darwin'; then ./sonarlint.sh; elif node -p 'process.platform' = 'win32'; then ./sonarlint.bat; else echo 'error'; fi",
    "storybook": "start-storybook -p 6006",
    "test": "cross-env NODE_ENV=test npm run test-unit && npm run eslint",
    "test-cover": "cross-env NODE_ENV=test nyc npm run test-unit",
    "test-unit": "cross-env NODE_ENV=test tape -r babel-register -r jsdom-global/register -r ./test/utils/stubStorage -r ./test/init.js src/**/*.unit.js src/**/unit.js | faucet",
    "test-unit-watch": "cross-env NODE_ENV=test tape-watch -r babel-register -r jsdom-global/register -r ./test/utils/stubStorage src/**/*.unit.js src/**/unit.js -p faucet"
  },
  "engines": {
    "node": "~8.2.0"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-3"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-babel",
    "cache": false,
    "exclude": [
      "**/*.unit.js",
      "**/unit.js",
      "**/stories.js",
      "public",
      "coverage"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary"
    ]
  },
  "author": "IBM",
  "license": "UNLICENSED",
  "peerDependencies": {
    "glamorous": ">=4",
    "prop-types": ">=15",
    "react": ">=16",
    "react-redux": ">=5",
    "react-router": ">=3",
    "redux": ">=3",
    "redux-form": ">=7"
  },
  "dependencies": {
    "react-collapse": "^4.0.3",
    "react-motion": "^0.5.2"
  },
  "devDependencies": {
    "@govuk-react/text-area": "^0.1.24",
    "@istanbuljs/nyc-config-babel": "^1.2.3",
    "@storybook/react": "^3.2.17",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.12.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-security": "^1.4.0",
    "expect": "^21.2.1",
    "faucet": "0.0.1",
    "file-loader": "^1.1.5",
    "glamorous": "^4.11.0",
    "govuk-colours": "^1.0.3",
    "govuk-elements-sass": "^3.1.2",
    "govuk_frontend_toolkit": "^7.2.0",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "make-index": "0.0.4",
    "node-sass": "^4.7.2",
    "nyc": "^11.5.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.1",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.2",
    "style-loader": "^0.19.0",
    "tape": "^4.8.0",
    "tape-watch": "^2.3.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.5"
  }
}
